{
  "name": "vue-universal-modal",
  "version": "1.1.3",
  "description": "Universal modal plugin for Vue@3",
  "license": "ISC",
  "author": {
    "name": "hoiheart",
    "email": "hoiheart@gmail.com",
    "url": "https://github.com/hoiheart"
  },
  "keywords": [
    "Vue",
    "VueJS",
    "Vue3",
    "modal",
    "Vue modal",
    "VueJS modal",
    "Vue3 modal"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hoiheart/vue-universal-modal.git"
  },
  "homepage": "https://github.com/hoiheart/vue-universal-modal#readme",
  "bugs": {
    "url": "https://github.com/hoiheart/vue-universal-modal/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "rimraf ./dist && rollup --config",
    "demo": "rimraf ./demo && vite build",
    "test": "vue-cli-service test:unit",
    "test:watch": "vue-cli-service test:unit --watch",
    "lint": "vue-cli-service lint"
  },
  "main": "dist/index.js",
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.14.2",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-typescript": "^6.1.0",
    "@semantic-release/git": "^9.0.0",
    "@types/jest": "^24.0.19",
    "@types/jsdom": "^16.2.5",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "@vitejs/plugin-vue": "^1.2.1",
    "@vue/cli-plugin-babel": "^4.5.9",
    "@vue/cli-plugin-eslint": "^4.5.9",
    "@vue/cli-plugin-typescript": "^4.5.9",
    "@vue/cli-plugin-unit-jest": "^4.5.9",
    "@vue/cli-service": "^4.5.9",
    "@vue/compiler-sfc": "^3.1.5",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/server-renderer": "^3.1.5",
    "@vue/test-utils": "^2.0.0-rc.4",
    "autoprefixer": "^8.6.5",
    "babel-eslint": "^10.1.0",
    "conventional-changelog-conventionalcommits": "^4.5.0",
    "core-js": "^3.8.0",
    "eslint": "^7.14.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^7.1.0",
    "jsdom": "^16.4.0",
    "lint-staged": "^10.5.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.34.0",
    "rollup-plugin-css-only": "^2.1.0",
    "rollup-plugin-typescript2": "^0.29.0",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.29.0",
    "sass-loader": "^8.0.2",
    "typescript": "^4.2.4",
    "vite": "^2.1.5",
    "vue": "^3.1.5",
    "vue-jest": "^5.0.0-alpha.8"
  },
  "lint-staged": {
    "src/**/*.{vue,ts,tsx}": [
      "vue-cli-service lint"
    ]
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "release": {
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "releaseRules": [
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "perf",
              "release": "patch"
            },
            {
              "type": "docs",
              "release": "patch"
            },
            {
              "type": "style",
              "release": "patch"
            }
          ]
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits",
          "presetConfig": {
            "types": [
              {
                "type": "feat",
                "section": "Features"
              },
              {
                "type": "fix",
                "section": "Bug Fixes"
              },
              {
                "type": "chore",
                "hidden": true
              },
              {
                "type": "docs",
                "hidden": false,
                "section": "Docs"
              },
              {
                "type": "style",
                "hidden": false,
                "section": "Style"
              },
              {
                "type": "refactor",
                "hidden": false,
                "section": "Refactor"
              },
              {
                "type": "perf",
                "hidden": false,
                "section": "Perfomance"
              },
              {
                "type": "test",
                "hidden": true
              }
            ]
          }
        }
      ],
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "dist",
            "demo",
            "package.json",
            "package-lock.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  }
}
